{
  "name": "worldforge-data",
  "version": "3.5.5",
  "description": "Worldforge data and OPS ecosystem â€” API + Archivist Cloudflare Workers.",
  "type": "module",
  "private": true,
  "scripts": {
    "dev:api": "cd worldforge-api && wrangler dev",
    "dev:archivist": "cd worldforge-archivist && wrangler dev",
    "deploy:api": "cd worldforge-api && wrangler deploy",
    "deploy:archivist": "cd worldforge-archivist && wrangler deploy",
    "deploy:all": "npm run deploy:api && npm run deploy:archivist",
    "kv:list": "wrangler kv namespaces list",
    "kv:inspect": "wrangler kv key list --namespace-id=$KV_NAMESPACE_ID",
    "lint": "eslint . --ext .ts,.js",
    "format": "prettier --write ."
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "undici": "^6.19.8"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "wrangler": "^4.46.0",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Hydremia / Worldforge OPS",
  "license": "MIT"
}
